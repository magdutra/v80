TYPE   COMPAREVALUE = (LESS, EQUAL, GREATER); FUNCTION LEN(S : STRING) : INTEGER; EXTERNAL;FUNCTION LEFT$(S : STRING; POSITION : INTEGER) : STRING; EXTERNAL;FUNCTION RIGHT$(S : STRING; POSITION : INTEGER) : STRING; EXTERNAL;FUNCTION MID$(S : STRING; POSITION, LENGTH : INTEGER) : STRING;         EXTERNAL;FUNCTION STR$(LENGTH : INTEGER; CH : CHAR) : STRING; EXTERNAL;FUNCTION ENCODEI(N : INTEGER) : STRING; EXTERNAL;FUNCTION ENCODER(R : REAL) : STRING; EXTERNAL;FUNCTION ENCODED(R : REAL (*DOUBLE PRECISION*)) : STRING; EXTERNAL;FUNCTION DECODEI(S : STRING) : INTEGER; EXTERNAL;FUNCTION DECODER(S : STRING) : REAL; EXTERNAL;FUNCTION DECODED(S : STRING) : REAL (*DOUBLE PRECISION*); EXTERNAL;FUNCTION CHARACTER(S : STRING; POSITION : INTEGER) : CHAR; EXTERNAL;FUNCTION CMPSTR(S1, S2 : STRING) : COMPAREVALUE; EXTERNAL;FUNCTION CONC(S1, S2 : STRING) : STRING; EXTERNAL;FUNCTION CPYSTR(S : STRING) : STRING; EXTERNAL;FUNCTION DELETE(S : STRING; POSITION, LENGTH : INTEGER) : STRING;         EXTERNAL;FUNCTION FIND(SUBSTRING, S : STRING) : INTEGER; EXTERNAL;FUNCTION INSERT(SUBSTRING, S : STRING; POSITION : INTEGER) : STRING;         EXTERNAL;FUNCTION REPLACE(OLDSTRING, NEWSTRING, S : STRING) : STRING; EXTERNAL;