10 CLS20 CLEAR 500025 LINE INPUT ME$30 OPEN "I",1,"SUPERD1/DAT"40 IF EOF(1) THEN CLOSE 1:GOTO 10050 LINE INPUT#1,A$60 IF INSTR(A$,ME$)<>0 THEN PRINT A$70 GOTO 40100 OPEN "I",2,"SUPERD2/DAT"110 IF EOF(2) THEN CLOSE 2:END120 LINE INPUT#1,A$130 IF INSTR(A$,ME$)<>0 THEN PRINT A$140 GOTO 110